<?xml version="1.0"?>

<plugin name="CDDB Plugin" version="1_0" reqversion="1_0">

  <source>plugins.cddb.CDDBPlugin</source>

  <description>
    Mit dem CDDB Plugin koennen zu einer beliebigen Musik CD
    die entsprechen Daten über Interpret, Album etc. automatisch
    über das Internet abgerufen werden.
  </description>

  <mount>
    <onEvent id="prenew"/>
  </mount>

  <prop key="titlefieldname"    value="Titel" />
  <prop key="categoryfieldname" value="Kategorie" />
  <prop key="artistfieldname"   value="Interpret" />
  
  <prop key="trackentityname"      value="Songs" />
  <prop key="compositionfieldname" value="Komposition" />
  <prop key="descriptionfieldname" value="Beschreibung" />
  <prop key="lengthfieldname"      value="Laenge" />
  <prop key="titlefieldname"       value="Titel" />
  <prop key="tracknofieldname"     value="TrackNr" />
  <prop key="cdfieldname"          value="CDName" />

</plugin>
